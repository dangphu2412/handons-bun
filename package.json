{
  "name": "bun-demo",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/index.ts",
    "start:dev": "bun --watch run src/index.ts",
    "build": "bun build --minify-whitespace --minify-syntax --sourcemap --compile --bytecode src/index.ts --outfile dist"
  },
  "devDependencies": {
    "@types/bun": "^1.2.11",
    "@types/figlet": "^1.7.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/jwt": "^1.2.0",
    "elysia": "^1.2.25",
    "figlet": "^1.8.1"
  }
}
